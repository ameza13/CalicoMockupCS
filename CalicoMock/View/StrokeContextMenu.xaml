<Window x:Class="CalicoMock.View.StrokeContextMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="StrokeContextMenu" SizeToContent="WidthAndHeight"
        WindowStyle="none"
        WindowStartupLocation="Manual"
        Topmost="True"
        AllowsTransparency="True"
        ResizeMode="NoResize"
        Background="Transparent">
    <Grid>
        <WrapPanel>
            <Button Name="cboCopyStroke" Click="cboCopyStroke_Click">
                <Image Source="..\Images\Copy.png" Stretch="None"></Image>
            </Button>
        </WrapPanel>
    </Grid>
</Window>
